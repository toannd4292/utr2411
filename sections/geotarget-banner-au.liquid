        banner.innerHTML = `
          Take advantage of our best-ever Power Subscription pricing! 
          Tennis Australia registrants can upgrade to an annual Power Subscription for only ${displayMonthlyAmount}/month!
        `;
        return;
      }

      if ((countryCode === "AU" || countryCode === "AUS") && !isTennisAustraliaMember) {
        console.log("[CASE] üá¶üá∫ AU Non-Member detected");
        const ausPlansUrl = "https://api.utrsports.net/v1/subscriptions/power/plans/australia";
        const ausRes = await fetch(ausPlansUrl);
        const ausPlans = await ausRes.json();

        const displayAnnualAmountRegular = ausPlans?.regularPlans?.[0]?.displayMonthlyAmount;
        const displayAnnualAmountTennis = ausPlans?.tennisAustraliaPlans?.[0]?.displayMonthlyAmount;

        headingFirst.innerHTML = `Upgrade for a superior`
