<script>
    window.__geoInfoPromise = window.__geoInfoPromise || null;

    window.getGeoInfo = function () {
        if (window.__geoInfoPromise) {
            return window.__geoInfoPromise;
        }

        const cached = localStorage.getItem('geoInfo');
        if (cached) {
            try {
                const parsed = JSON.parse(cached);
                if (parsed && parsed.region_code) { 
                    window.__geoInfoPromise = Promise.resolve(parsed);
                    return window.__geoInfoPromise;
                } else {
                    localStorage.removeItem('geoInfo');
                }
            } catch (e) {
                localStorage.removeItem('geoInfo');
            }
        }

        const accessKey = "{{ settings.key_ipstack }}";

        window.__geoInfoPromise = fetch(`https://api.ipstack.com/check?access_key=${accessKey}`)
            .then(response => response.json())
            .then(data => {
                if (data && data.region_code) {
                    localStorage.setItem('geoInfo', JSON.stringify(data));
                    return data;
                } else {
                    throw new Error("Invalid geoInfo: missing region_code");
                }
            })
            .catch(error => {
                localStorage.removeItem('geoInfo');
                window.__geoInfoPromise = null;
                throw error;
            });

        return window.__geoInfoPromise;
    };

    async function getUserCountryCodeFallback() {
        try {
            const geoInfo = await window.getGeoInfo();
            if (geoInfo?.region_code) return geoInfo.region_code.toUpperCase();
        } catch (err) {
            console.warn("Kh√¥ng th·ªÉ l·∫•y geoInfo fallback:", err);
        }
        return null;
    }

    {% assign cleanSectionID = section.id | replace: '-', '' | replace: '_', '' %}
    function section{{ cleanSectionID }}() {
        let countryCode = "{{ section.settings.country_code-au }}";
        let hideCountry = "{{ section.settings.hide_country_code-au }}";

        let countryCodeArray = countryCode.replace(/,/g, '')
            .split(' ')
            .map(code => code.trim().toUpperCase())
            .filter(code => code);

        let hideCountryArray = hideCountry.replace(/,/g, '')
            .split(' ')
            .map(code => code.trim().toUpperCase())
            .filter(code => code);

        window.getGeoInfo().then(geoInfo => {
            if (!geoInfo || !geoInfo.region_code) return;

            const userCountryCode = geoInfo.region_code.toUpperCase();
            const watchLiveElement = document.getElementById('shopify-section-{{ section.id }}');

            if (watchLiveElement) {
                if (countryCodeArray.length === 0 && hideCountryArray.length === 0) {
                    watchLiveElement.style.display = 'block';
                } else if (countryCodeArray.includes(userCountryCode)) {
                    watchLiveElement.style.display = 'block';
                } else if (hideCountryArray.includes(userCountryCode)) {
                    watchLiveElement.style.display = 'none';
                } else {
                    watchLiveElement.style.display = countryCodeArray.length > 0 ? 'none' : 'block';
                }
            }
        }).catch(err => {
            const watchLiveElement = document.getElementById('shopify-section-{{ section.id }}');
            if (watchLiveElement) {
                watchLiveElement.style.display = 'block';
            }
        });
    }

    document.addEventListener("DOMContentLoaded", function () {
        section{{ cleanSectionID }}();
    });
    
  async function checkUserAuthWithGeo() {
    const banner = document.getElementById("powerBanner-{{ cleanSectionID }}");
    const headingBanner = document.getElementById("heading-banner-{{ cleanSectionID }}");
    const btnNonTA = document.getElementById("show-btn-{{ cleanSectionID }}");
    const headingFirst = document.getElementById("heading-first-{{ cleanSectionID }}");
    const headingSecond = document.getElementById("heading-second-{{ cleanSectionID }}");
    let jwt = null;
    let currentData = null;
    let plansData = null;
    let countryCode = null;
    let residencyCountryName = null;

    try {
      // 1Ô∏è‚É£ Call /auth/current to get user information & JWT
      const response = await fetch("https://api.utrsports.net/v1/auth/current", {
        method: "GET",
        credentials: "include",
        headers: { "Accept": "application/json" }
      });

      console.log("HTTP status:", response.status);

      if (response.status === 200) {
        currentData = await response.json();
        console.log("‚úÖ [AUTH CURRENT DATA]", currentData);

        jwt = document.cookie.split("; ").find(row => row.startsWith("jwt="))?.split("=")[1] || null;
        console.log("üîë [JWT FOUND]", jwt ? "‚úÖ" : "‚ùå");

        if (currentData?.isPowered === true) {
          console.log("[CASE] Powered user");
          headingFirst.innerHTML = `Enjoy your superior `
          headingSecond.innerHTML = ` playing experience`
          banner.innerHTML = `
            Take advantage of your UTR Sports Power Subscription and track your progress, 
            scout your opponents, and find better local matches. 
            You also can access advanced analytics and save money on every verified UTR Sports tennis event you play!
          `;
          return;
        }
      }
    } catch (err) {
      console.warn("‚ö†Ô∏è [AUTH CURRENT FAILED]", err);
    }

    // 2Ô∏è‚É£ Without JWT ‚Üí Try geoInfo or ipstack before defaulting to Australia
    if (!jwt) {
      console.log("[CASE] No JWT ‚Üí checking GeoInfo or IPStack for user country");

      let geoData = null;
      countryCode = "AU"; // default fallback

      try {
        const storedGeo = localStorage.getItem("geoInfo");
        if (storedGeo) {
          geoData = JSON.parse(storedGeo);
          countryCode = geoData?.country_code || geoData?.countryCode || "AU";
          console.log("üìç [GeoInfo from localStorage]", countryCode);
        } else {
          const ipRes = await fetch("https://api.ipstack.com/check?access_key={{ settings.key_ipstack }}");
          const ipJson = await ipRes.json();
          geoData = ipJson;
          countryCode = geoData?.country_code || "AU";
          console.log("üìç [GeoInfo from ipstack]", countryCode);
          localStorage.setItem("geoInfo", JSON.stringify(geoData));
        }
      } catch (geoErr) {
        console.warn("‚ö†Ô∏è [Geo lookup failed ‚Üí fallback AU]", geoErr);
      }

      console.log(`üåé [VISITOR COUNTRY DETECTED]: ${countryCode}`);

      if (countryCode === "AU" || countryCode === "AUS") {
        console.log("[CASE] Visitor from Australia ‚Üí Show AU plans");
        const ausPlansUrl = "https://api.utrsports.net/v1/subscriptions/power/plans/australia";
        const ausRes = await fetch(ausPlansUrl);
        const ausPlans = await ausRes.json();

        const displayAnnualAmountRegular = ausPlans?.regularPlans?.[0]?.displayMonthlyAmount;
        const displayAnnualAmountTennis = ausPlans?.tennisAustraliaPlans?.[0]?.displayMonthlyAmount;
        headingFirst.innerHTML = `Upgrade for a superior `
        headingSecond.innerHTML = ` playing experience`
        banner.innerHTML = `
          Take advantage of our best-ever Power Subscription pricing! 
          In Australia, upgrade to an annual Power Subscription for only <strong>${displayAnnualAmountRegular}/month</strong>. 
          Tennis Australia registrants can save even more and upgrade to annual for only <strong>${displayAnnualAmountTennis}/month</strong>!
        `;

        if (btnNonTA) btnNonTA.style.display = "none";
        return;
      }

      // üåç Other countries
      console.log("[CASE] Visitor from outside AU ‚Üí Show generic plans");
      const localUrl = "https://api.utrsports.net/v1/subscriptions/power/plans/australia";
      const localRes = await fetch(localUrl);
      const localPlans = await localRes.json();

      const displayAnnualAmountRegular = localPlans?.regularPlans?.[0]?.displayMonthlyAmount;
      const displayAnnualAmountTennis = localPlans?.tennisAustraliaPlans?.[0]?.displayMonthlyAmount;
      headingFirst.innerHTML = `Upgrade for a superior`
      headingSecond.innerHTML = ` playing experience`
      banner.innerHTML = `
        Take advantage of our best-ever Power Subscription pricing! 
        In Australia, upgrade to an annual Power Subscription for only <strong>${displayAnnualAmountRegular}/month</strong>. 
        Tennis Australia registrants can save even more and upgrade to annual for only <strong>${displayAnnualAmountTennis}/month</strong>!
      `;
      if (btnNonTA) btnNonTA.style.display = "block";
      return;
    }

    // 3Ô∏è‚É£ There is JWT ‚Üí Call API to get Power Plans information
    try {
      const plansUrl = "https://api.utrsports.net/v1/subscriptions/power/plans";
      const plansRes = await fetch(plansUrl, { headers: { "Authorization": `Bearer ${jwt}` } });
      plansData = await plansRes.json();
      console.log("üì¶ [POWER PLANS DATA]", plansData);

      const isTennisAustraliaMember = plansData?.isTennisAustraliaMember === true;
      countryCode = currentData?.location?.countryCode || "US";
      residencyCountryName = currentData?.location?.residencyCountryName || "your location";
      console.log("üåè [USER COUNTRY]", countryCode);

      if (isTennisAustraliaMember) {
        console.log("[CASE] Tennis Australia Member");
        const ausPlansUrl = "https://api.utrsports.net/v1/subscriptions/power/plans/australia";
